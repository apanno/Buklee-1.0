[32m+[m[32m<?php[m
[32m+[m[32m/**[m
[32m+[m[32m * The template for displaying comments.[m
[32m+[m[32m *[m
[32m+[m[32m * This is the template that displays the area of the page that contains both the current comments[m
[32m+[m[32m * and the comment form.[m
[32m+[m[32m *[m
[32m+[m[32m * @link https://codex.wordpress.org/Template_Hierarchy[m
[32m+[m[32m *[m
[32m+[m[32m * @package seed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Disabled by SeedThemes. We are not recommended using WordPress comments.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreturn;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * If the current post is protected by a password and[m
[32m+[m[32m * the visitor has not yet entered the password we will[m
[32m+[m[32m * return early without loading the comments.[m
[32m+[m[32m */[m
[32m+[m[32mif ( post_password_required() ) {[m
[32m+[m	[32mreturn;[m
[32m+[m[32m}[m
[32m+[m[32m?>[m
[32m+[m
[32m+[m[32m<div id="comments" class="comments-area">[m
[32m+[m
[32m+[m	[32m<?php // You can start editing here -- including this comment! ?>[m
[32m+[m
[32m+[m	[32m<?php if ( have_comments() ) : ?>[m
[32m+[m		[32m<h2 class="comments-title">[m
[32m+[m			[32m<?php[m
[32m+[m				[32mprintf( // WPCS: XSS OK.[m
[32m+[m					[32mesc_html( _nx( 'One thought on &ldquo;%2$s&rdquo;', '%1$s thoughts on &ldquo;%2$s&rdquo;', get_comments_number(), 'comments title', 'seed' ) ),[m
[32m+[m					[32mnumber_format_i18n( get_comments_number() ),[m
[32m+[m					[32m'<span>' . get_the_title() . '</span>'[m
[32m+[m				[32m);[m
[32m+[m			[32m?>[m
[32m+[m		[32m</h2>[m
[32m+[m
[32m+[m		[32m<?php if ( get_comment_pages_count() > 1 && get_option( 'page_comments' ) ) : // Are there comments to navigate through? ?>[m
[32m+[m		[32m<nav id="comment-nav-above" class="navigation comment-navigation" role="navigation">[m
[32m+[m			[32m<h2 class="screen-reader-text"><?php esc_html_e( 'Comment navigation', 'seed' ); ?></h2>[m
[32m+[m			[32m<div class="nav-links">[m
[32m+[m
[32m+[m				[32m<div class="nav-previous"><?php previous_comments_link( esc_html__( 'Older Comments', 'seed' ) ); ?></div>[m
[32m+[m				[32m<div class="nav-next"><?php next_comments_link( esc_html__( 'Newer Comments', 'seed' ) ); ?></div>[m
[32m+[m
[32m+[m			[32m</div><!-- .nav-links -->[m
[32m+[m		[32m</nav><!-- #comment-nav-above -->[m
[32m+[m		[32m<?php endif; // Check for comment navigation. ?>[m
[32m+[m
[32m+[m		[32m<ol class="comment-list">[m
[32m+[m			[32m<?php[m
[32m+[m				[32mwp_list_comments( array([m
[32m+[m					[32m'style'      => 'ol',[m
[32m+[m					[32m'short_ping' => true,[m
[32m+[m				[32m) );[m
[32m+[m			[32m?>[m
[32m+[m		[32m</ol><!-- .comment-list -->[m
[32m+[m
[32m+[m		[32m<?php if ( get_comment_pages_count() > 1 && get_option( 'page_comments' ) ) : // Are there comments to navigate through? ?>[m
[32m+[m		[32m<nav id="comment-nav-below" class="navigation comment-navigation" role="navigation">[m
[32m+[m			[32m<h2 class="screen-reader-text"><?php esc_html_e( 'Comment navigation', 'seed' ); ?></h2>[m
[32m+[m			[32m<div class="nav-links">[m
[32m+[m
[32m+[m				[32m<div class="nav-previous"><?php previous_comments_link( esc_html__( 'Older Comments', 'seed' ) ); ?></div>[m
[32m+[m				[32m<div class="nav-next"><?php next_comments_link( esc_html__( 'Newer Comments', 'seed' ) ); ?></div>[m
[32m+[m
[32m+[m			[32m</div><!-- .nav-links -->[m
[32m+[m		[32m</nav><!-- #comment-nav-below -->[m
[32m+[m		[32m<?php endif; // Check for comment navigation. ?>[m
[32m+[m
[32m+[m	[32m<?php endif; // Check for have_comments(). ?>[m
[32m+[m
[32m+[m	[32m<?php[m
[32m+[m		[32m// If comments are closed and there are comments, let's leave a little note, shall we?[m
[32m+[m		[32mif ( ! comments_open() && get_comments_number() && post_type_supports( get_post_type(), 'comments' ) ) :[m
[32m+[m	[32m?>[m
[32m+[m		[32m<p class="no-comments"><?php esc_html_e( 'Comments are closed.', 'seed' ); ?></p>[m
[32m+[m	[32m<?php endif; ?>[m
[32m+[m
[32m+[m	[32m<?php comment_form(); ?>[m
[32m+[m
[32m+[m[32m</div><!-- #comments -->[m
