[32m+[m[32m<?php[m
[32m+[m[32m/**[m
[32m+[m[32m * The template for displaying archive pages.[m
[32m+[m[32m *[m
[32m+[m[32m * @link https://codex.wordpress.org/Template_Hierarchy[m
[32m+[m[32m *[m
[32m+[m[32m * @package seed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mget_header(); ?>[m
[32m+[m
[32m+[m[32m<div class="main-header">[m
[32m+[m	[32m<div class="container">[m
[32m+[m		[32m<?php the_archive_title( '<h2 class="main-title">', '</h2>' ); ?>[m
[32m+[m	[32m</div>[m
[32m+[m[32m</div>[m
[32m+[m
[32m+[m[32m<div class="container">[m
[32m+[m	[32m<div id="primary" class="content-area <?php echo '-'.SEED_BLOG_LAYOUT; ?>">[m
[32m+[m		[32m<main id="main" class="site-main" role="main">[m
[32m+[m
[32m+[m			[32m<?php if ( have_posts() ) : ?>[m
[32m+[m
[32m+[m				[32m<header class="page-header">[m
[32m+[m					[32m<?php[m
[32m+[m					[32mthe_archive_title( '<h1 class="page-title entry-title hide">', '</h1>' );[m
[32m+[m					[32mthe_archive_description( '<div class="taxonomy-description">', '</div>' );[m
[32m+[m					[32m?>[m
[32m+[m				[32m</header><!-- .page-header -->[m
[32m+[m
[32m+[m				[32m<?php[m[41m [m
[32m+[m				[32mif ((int)SEED_BLOG_COLUMNS > 1) {[m
[32m+[m					[32mecho '<div class="seed-grid-'.SEED_BLOG_COLUMNS.'">';[m
[32m+[m					[32mwhile ( have_posts() ) : the_post();[m
[32m+[m					[32mget_template_part( 'template-parts/content','card-excerpt');[m
[32m+[m					[32mendwhile;[m[41m [m
[32m+[m					[32mecho '</div>';[m
[32m+[m				[32m} else {[m
[32m+[m					[32mwhile ( have_posts() ) : the_post();[m
[32m+[m					[32mget_template_part( 'template-parts/content');[m
[32m+[m					[32mendwhile;[m[41m [m
[32m+[m				[32m}[m
[32m+[m				[32m?>[m
[32m+[m
[32m+[m				[32m<?php seed_posts_navigation(); ?>[m
[32m+[m
[32m+[m			[32m<?php else : ?>[m
[32m+[m
[32m+[m				[32m<?php get_template_part( 'template-parts/content', 'none' ); ?>[m
[32m+[m
[32m+[m			[32m<?php endif; ?>[m
[32m+[m
[32m+[m		[32m</main><!-- #main -->[m
[32m+[m	[32m</div><!-- #primary -->[m
[32m+[m
[32m+[m	[32m<?php[m[41m [m
[32m+[m	[32mswitch (SEED_BLOG_LAYOUT) {[m
[32m+[m		[32mcase 'rightbar':[m
[32m+[m		[32mget_sidebar('right');[m[41m [m
[32m+[m		[32mbreak;[m
[32m+[m		[32mcase 'leftbar':[m
[32m+[m		[32mget_sidebar('left');[m[41m [m
[32m+[m		[32mbreak;[m
[32m+[m		[32mcase 'full-width':[m
[32m+[m		[32mbreak;[m
[32m+[m		[32mdefault:[m
[32m+[m		[32mbreak;[m
[32m+[m	[32m}[m
[32m+[m	[32m?>[m
[32m+[m[32m</div><!--container-->[m
[32m+[m[32m<?php get_footer(); ?>[m
